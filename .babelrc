{
  "presets": [
    "@babel/preset-typescript",
    [
      "next/babel",
      {
        "preset-env": {
          "modules": "commonjs"
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "@app": "./src",
          "@assets": "./public/static/"
        }
      }
    ]
  ]
}